{
  "name": "mock-user-auth",
  "version": "1.0.13",
  "description": "Mock-User-Auth is a mock user authentication API developed in Nodejs and Express using JWT as an authenticator",
  "private": false,
  "author": "Thiago Luiz Nunes <thiagoluiz.dev@gmail.com> (https://github.com/thiagoluiznunes)",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "nodemon ./node_modules/mock-user-auth/bin/www.js",
    "build": "scripts/build.sh",
    "start": "node app.js",
    "lint": "scripts/lint.sh",
    "test": "jest",
    "coverage": "nyc --reporter=lcov npm test"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.4",
    "body-parser": "^1.20.3",
    "debug": "~2.6.9",
    "express": "^4.21.1",
    "faker": "4.1.0",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.0",
    "nodemon": "^3.1.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/node": "7.4.5",
    "@babel/preset-env": "^7.26.0",
    "@babel/register": "^7.25.9",
    "babel-jest": "^29.7.0",
    "babel-plugin-istanbul": "5.1.4",
    "babel-polyfill": "6.26.0",
    "chai": "4.2.0",
    "chai-http": "4.2.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.3",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "mocha": "^10.7.3",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "^17.1.0",
    "supertest": "^7.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(your-module-to-transform)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testEnvironment": "node",
    "reporters": [
      "default",
      ["jest-html-reporters", {
        "pageTitle": "Test Report",
        "outputPath": "reports/test-report.html",
        "includeFailureMsg": true,
        "includeConsoleLog": true
      }]
    ]
  },
  "engines": {
    "node": "11.x",
    "npm": "6.x"
  },
  "keywords": [
    "JSON",
    "server",
    "express",
    "node",
    "http",
    "fake",
    "REST",
    "API",
    "prototyping",
    "mock",
    "mocking",
    "test",
    "testing",
    "rest",
    "data",
    "json-server",
    "middleware",
    "auth",
    "authentication",
    "authorization",
    "jwt",
    "chai",
    "mocha",
    "istanbul",
    "babel",
    "coverage"
  ],
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThiagoLuizNunes/mock-json-server.git"
  },
  "bugs": {
    "url": "https://github.com/ThiagoLuizNunes/mock-json-server/issues"
  },
  "homepage": "https://github.com/ThiagoLuizNunes/mock-json-server#readme"
}
